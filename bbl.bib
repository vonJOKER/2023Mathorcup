@phdthesis{__2015-1,
	type = {硕士},
	title = {区域性应急物流网络节点能力配置的研究},
	url = {https://kns.cnki.net/KCMS/detail/detail.aspx?dbcode=CMFD&dbname=CMFD201502&filename=1015611495.nh&v=},
	abstract = {无论是事故灾害、公共卫生,还是社会安全等的应急事件发生,都需要将大量的应急物资以最快的方式送达受灾地区,以救助伤者、灾后重建和恢复生产等,否则将带来不可估量的损失。因为应急事件很难有预测和预报,又由于灾民需要的各种应急物资在全国各地的应急储备库中储备,如果没有能够有效合理对区域性应急物流网络节点能力进行有效的配置,其时间效益和空间效益就没办法得到实现,这不仅会耽误对灾区人民应急救援的最佳时间,更会使灾区人民的生命财产遭受到更大的损失,而且还会影响社会稳定,对经济社会发展全局产生重大影响。本文在参阅国内外文献的基础上,以区域性应急物流网络节点物流能力为研究对象,对区域性应急物流节点能力进行定量分析和优化配置研究。首先在应急物流和应急节点能力基本理论的基础上,提出了区域性应急物流节点能力的概念和构成,同时研究了应急物流节点的概念、功能和分类,并深入分析了对区域性应急物流节点能力的内外部影响因素。其次从区域性应急物流网络节点能力构成要素的角度对区域性应急物流网络节点能力的评价指标进行研究,然后建立了区域性应急物流网络节点物流能力的计算模型。第三,在计算完区域性应急物流网络节点能力后要对各个应急节点进行优化配置,在应急物流节点处需要多少救援人员和不同效率叉车的数量才能使应急物流节点处的能力配置最优,在模型求解时,应该在保证救援资源在时间一定的情况下,使用的成本最小。最后引入案例,验证了构建的应急节点物流能力的计算模型和节点物流能力的配置优化。},
	language = {zh-CN},
	school = {北京交通大学},
	author = {赵, 越},
	collaborator = {赵, 启兰},
	year = {2015},
	note = {3 citations(CNKI)[2023-4-17]},
	keywords = {节点能力 Emergency logistics, 应急节点, 应急物流, 应急物流网络, Emergency logistics network, Emergency nodes, The capacity of node},
	file = {区域性应急物流网络节点能力配置的研究_赵越.pdf:C\:\\Users\\yuhangmeng\\Zotero\\storage\\TF56AIPZ\\区域性应急物流网络节点能力配置的研究_赵越.pdf:application/pdf},
}
